{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyammarcano/electron-angular2-scaffold.git"
  },
  "scripts": {
    "rimraf": "rimraf",
    "view:dev": "electron --debug src/ -r",
    "view:prod": "electron dist/",
    "build": "npm run clean && gulp build",
    "clean": "npm run rimraf -- dist package",
    "package:all": "npm run build && npm run package:linux && npm run package:mac && npm run package:win",
    "package:linux": "electron-packager ./dist --asar --platform=linux --arch=all --out=./package/linux --icon=icon/png/app.png --overwrite --prune --version='1.3.1' --tmpdir=false",
    "package:mac": "electron-packager ./dist --asar --platform=darwin --arch=all --out=./package/mac --icon=icon/app.icns --overwrite --prune --version='1.3.1' --tmpdir=false",
    "package:win": "electron-packager ./dist --asar --platform=win32 --arch=all --out=./package/win --icon=icon/app.ico --overwrite --prune --version='1.3.1' --tmpdir=false"
  },
  "author": "Dyam Marcano <dyam.marcano@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.2.2",
    "del": "^2.2.1",
