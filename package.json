    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.1",
    "html-loader": "^0.4.3",
    "node-sass": "^3.4.2",
    "remote": "^0.2.6",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "spectron": "^3.3.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "unminified-webpack-plugin": "^1.1.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^2.1.0-beta.8",
